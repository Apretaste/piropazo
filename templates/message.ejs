<% if(navigation) { %>
	<div class="row">
		<div class="col s12">
			<ul class="tabs tabs-fixed-width">
				<li class="tab"><a href="#" onclick="apretaste.send({command: 'PIROPAZO CITAS'})">Citas</a></li>
				<li class="tab"><a href="#" onclick="apretaste.send({command: 'PIROPAZO PAREJAS'})">Parejas</a></li>
				<li class="tab"><a href="#" onclick="apretaste.send({command: 'PIROPAZO PERFIL'})">Perfil</a></li>
				<li class="tab"><a href="#" onclick="apretaste.send({command: 'PIROPAZO TIENDA'})">Tienda</a></li>
				<li class="tab hide"><a href="#" class="active">Hidden to make all tabs inactive</a></li>
			</ul>
		</div>
	</div>
<% } %>

<div class="row">
	<div class="col s12 center">
		<h1 class="black-text"><%= header %></h1>
		<i class="material-icons large"><%= icon %></i>
		<p><%= text %></p>
		<a class="waves-effect waves-light btn green" href="#!" onclick="apretaste.send({'command':'<%= button['href'] %>'})">
			<%= button['caption'] %>
		</a>
	</div>
</div>
